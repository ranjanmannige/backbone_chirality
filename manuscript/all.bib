@Article{Sen2004,
  Title                    = {Predicting binding sites of hydrolase-inhibitor complexes by combining several methods.},
  Author                   = {Taner Z Sen and Andrzej Kloczkowski and Robert L Jernigan and Changhui Yan and Vasant Honavar and Kai-Ming Ho and Cai-Zhuang Wang and Yungok Ihm and Haibo Cao and Xun Gu and Drena Dobbs},
