@Article{Nam2010,
  Title                    = {Free-floating ultrathin two-dimensional crystals from sequence-specific peptoid polymers.},
  Author                   = {Nam, Ki Tae and Shelby, Sarah A. and Choi, Philip H. and Marciel, Amanda B. and Chen, Ritchie and Tan, Li and Chu, Tammy K. and Mesch, Ryan A. and Lee, Byoung-Chul and Connolly, Michael D. and Kisielowski, Christian and Zuckermann, Ronald N.},
