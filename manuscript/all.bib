@Article{Haynes2006,
  Title                    = {Intrinsic disorder is a common feature of hub proteins from four eukaryotic interactomes.},
  Author                   = {Chad Haynes and Christopher J Oldfield and Fei Ji and Niels Klitgord and Michael E Cusick and Predrag Radivojac and Vladimir N Uversky and Marc Vidal and Lilia M Iakoucheva},
  Journal                  = {PLoS Comput Biol},
