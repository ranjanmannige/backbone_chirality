@Article{Guvench2011,
  Title                    = {CHARMM additive all-atom force field for carbohydrate derivatives and its utility in polysaccharide and carbohydrate-protein modeling.},
  Author                   = {Guvench, Olgun and Mallajosyula, Sairam S. and Raman, E Prabhu and Hatcher, Elizabeth and Vanommeslaeghe, Kenno and Foster, Theresa J. and Jamison, 2nd, Francis W and Mackerell, Jr, Alexander D},
