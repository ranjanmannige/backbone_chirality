  Pmid                     = {23028272},
  Timestamp                = {2013.06.13}
}

@Article{Sim2008,
  Title                    = {The core and accessory genomes of Burkholderia pseudomallei: implications for human melioidosis.},
  Author                   = {Sim, Siew Hoon and Yu, Yiting and Lin, Chi Ho and Karuturi, R Krishna M. and Wuthiekanun, Vanaporn and Tuanyok, Apichai and Chua, Hui Hoon and Ong, Catherine and Paramalingam, Sivalingam Suppiah and Tan, Gladys and Tang, Lynn and Lau, Gary and Ooi, Eng Eong and Woods, Donald and Feil, Edward and Peacock, Sharon J. and Tan, Patrick},
  Journal                  = {PLoS Pathog},
