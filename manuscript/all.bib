@Article{Kersey2010,
  Title                    = {Ensembl Genomes: extending Ensembl across the taxonomic space.},
  Author                   = {P. J. Kersey and D. Lawson and E. Birney and P. S. Derwent and M. Haimel and J. Herrero and S. Keenan and A. Kerhornou and G. Koscielny and A. K\"{a}h\"{a}ri and R. J. Kinsella and E. Kulesha and U. Maheswari and K. Megy and M. Nuhn and G. Proctor and D. Staines and F. Valentin and A. J. Vilella and A. Yates},
