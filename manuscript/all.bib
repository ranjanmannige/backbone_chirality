@Article{Oldfield2005,
  Title                    = {Comparing and combining predictors of mostly disordered proteins.},
  Author                   = {Christopher J Oldfield and Yugong Cheng and Marc S Cortese and Celeste J Brown and Vladimir N Uversky and A. Keith Dunker},
  Journal                  = {Biochemistry},
  Year                     = {2005},

