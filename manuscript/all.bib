@Article{Radivojac2004,
  Title                    = {Protein flexibility and intrinsic disorder.},
  Author                   = {Radivojac, Predrag and Obradovic, Zoran and Smith, David K. and Zhu, Guang and Vucetic, Slobodan and Brown, Celeste J. and Lawson, J David and Dunker, A Keith},
