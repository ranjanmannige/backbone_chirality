@Article{Sanii2011,
  Title                    = {Shaken, not stirred: collapsing a peptoid monolayer to produce free-floating, stable nanosheets.},
  Author                   = {Sanii, Babak and Kudirka, Romas and Cho, Andrew and Venkateswaran, Neeraja and Olivier, Gloria K. and Olson, Alexander M. and Tran, Helen and Harada, R Marika and Tan, Li and Zuckermann, Ronald N.},
  Journal                  = {J Am Chem Soc},
  Year                     = {2011},

  Month                    = {Dec},
  Number                   = {51},
  Pages                    = {20808--20815},
  Volume                   = {133},

  Abstract                 = {Two-dimensional nanomaterials play a critical role in biology (e.g., lipid bilayers) and electronics (e.g., graphene) but are difficult to directly synthesize with a high level of precision. Peptoid nanosheet bilayers are a versatile synthetic platform for constructing multifunctional, precisely ordered two-dimensional nanostructures. Here we show that nanosheet formation occurs through an unusual monolayer intermediate at the air-water interface. Lateral compression of a self-assembled peptoid monolayer beyond a critical collapse pressure results in the irreversible production of nanosheets. An unusual thermodynamic cycle is employed on a preparative scale, where mechanical energy is used to buckle an intermediate monolayer into a more stable nanosheet. Detailed physical studies of the monolayer-compression mechanism revealed a simple preparative technique to produce nanosheets in 95\% overall yield by cyclical monolayer compressions in a rotating closed vial. Compression of 
monolayers into stable, free-floating products may be a general and preparative approach to access 2D nanomaterials.},
  Doi                      = {10.1021/ja206199d},
  Institution              = {The Molecular Foundry, Lawrence Berkeley National Laboratory, 1 Cyclotron Road, Berkeley, California 94720, USA.},
  Keywords                 = {Air; Models, Molecular; Nanostructures, chemistry/ultrastructure; Nanotechnology, methods; Peptoids, chemistry; Thermodynamics; Water, chemistry},
  Medline-pst              = {ppublish},
  Owner                    = {ranjan},
