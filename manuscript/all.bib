@Article{Looger2001,
  Title                    = {Generalized dead-end elimination algorithms make large-scale protein side-chain structure prediction tractable: implications for protein design and structural genomics.},
  Author                   = {L. L. Looger and H. W. Hellinga},
  Journal                  = {J Mol Biol},
