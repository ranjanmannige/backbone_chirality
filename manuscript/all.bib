@Article{Nandhagopal2002,
  Title                    = {The structure and evolution of the major capsid protein of a large, lipid-containing DNA virus.},
  Author                   = {Narayanasamy Nandhagopal and Alan A Simpson and James R Gurnon and Xiadong Yan and Timothy S Baker and Michael V Graves and James L Van Etten and Michael G Rossmann},
